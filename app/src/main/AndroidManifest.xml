<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="set.dhy.com.dhyset">

    <uses-permission android:name="android.permission.USE_FINGERPRINT" />
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 声明所有需要的权限（包括普通权限和危险权限） -->
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".MyAppLication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RightSlideExit.RightSlideExitMainActivity"
            tools:ignore="InnerclassSeparator" />
        <activity
            android:name=".RightSlideExit.RightSlideBaseActivity"
            android:theme="@style/SwipeBackTheme"
            tools:ignore="InnerclassSeparator" />
        <activity android:name=".RightSlideExit.example.ExitOneActivity" />
        <activity android:name=".PrintPay.PrintPayActivity" />
        <activity android:name=".rvadapter.Examples.AllExampleActivity" />
        <activity android:name=".rvadapter.Examples.HomePageActivity" />
        <activity android:name=".rvadapter.Examples.GridActivity" />
        <activity android:name=".rvadapter.Examples.StateActivity" />
        <activity android:name=".PhotoView.PhotoViewActivity" />
        <activity android:name=".RecyclerViewItemDecoration.EasyShowTitleActivity" />
        <activity android:name=".BottomSheet.BottomSheetActivity" />
        <activity android:name=".JumpActivityAnims.JumpAnimActivity" />
        <activity
            android:name=".JumpActivityAnims.JumpToActivity"
            android:label="@string/title_activity_jump_to"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".JumpActivityAnims.ShareAnimActivity" />
        <activity android:name=".shuang_biao_tou.TwoTitleActivity" />
        <activity android:name=".WaterWave.WaveActivity"></activity>
    </application>

</manifest>